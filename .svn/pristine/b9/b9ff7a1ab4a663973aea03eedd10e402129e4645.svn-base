date-font\">验证码：</text>\r\n        <input\r\n          type=\"text\"\r\n          class=\"update-input\"\r\n          v-model=\"newCode\"\r\n          placeholder=\"请输入验证码\"\r\n        />\r\n      </view>\r\n      <button\r\n        type=\"text\"\r\n        :plain=\"true\"\r\n        size=\"mini\"\r\n        style=\"border: none\"\r\n        @click=\"getCode\"\r\n        class=\"phone-right\"\r\n        :disabled=\"disable\"\r\n        :class=\"{ codeGeting: isGeting }\"\r\n      >\r\n        {{ time }}\r\n      </button>\r\n    </view>\r\n    <view class=\"update-button\">\r\n      <image\r\n        src=\"../static/imgs/common/save.png\"\r\n        mode=\"aspectFill\"\r\n        @click=\"updatePhoneNumber\"\r\n      ></image>\r\n    </view>\r\n  </view>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      newPhone: \"\",\r\n      newCode: \"\",\r\n      time: \"获取验证码\",\r\n      count: 59,\r\n      isGeting: false,\r\n      disable: false,\r\n      countDown: null,\r\n    };\r\n  },\r\n  onReady() {\r\n    this.hidePageNavInWechatBrowser();\r\n  },\r\n  methods: {\r\n    // 修改手机号\r\n    updatePhoneNumber() {\r\n      if (this.newPhone == \"\") {\r\n        uni.showToast({\r\n          title: \"请输入新手机号\",\r\n          icon: \"none\",\r\n        });\r\n      } else if (this.newCode == \"\") {\r\n        uni.showToast({\r\n          title: \"请输入验证码\",\r\n          icon: \"none\",\r\n        });\r\n      } else {\r\n        uni.setStorageSync(\"newPhone\", this.newPhone);\r\n        uni.navigateTo({\r\n          url: \"./updateMyMsg\",\r\n        });\r\n      }\r\n    },\r\n    // 获取验证码\r\n    getCode() {\r\n      if (this.newPhone == \"\") {\r\n        uni.showToast({\r\n          title: \"请输入手机号/邮箱\",\r\n          icon: \"none\",\r\n        });\r\n      } else {\r\n        uni.showLoading({\r\n          title: \"验证码获取中...\",\r\n        });\r\n        uni.request({\r\n          method: \"GET\",\r\n          url: this.$baseUrl + \"/