<block wx:if="{{isShow}}"><view class="content"><view class="nav"><scroll-view class="tab-scroll" scroll-x="true" scroll-with-animation="{{true}}" scroll-left="{{scrollLeft}}"><view class="tab-scroll_box"><block wx:for="{{category}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['chenked',[index]]]]]}}" class="{{['tab-scroll_item',(isActive==index)?'active':'']}}" bindtap="__e">{{''+item.name+''}}</view></block></view></scroll-view></view><swiper class="swiper-content" style="{{(fullHeight)}}" current="{{isActive}}" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><block wx:for="{{$root.l2}}" wx:for-item="item2" wx:for-index="index2" wx:key="index2"><swiper-item class="swiperitem-content"><scroll-view style="height:100%;" scroll-y="{{true}}"><view class="nav_item"><block wx:if="{{item2.$orig.orderList.length==0}}"><view class="no-data"><image src="../static/imgs/common/no.png" mode="aspectFill"></image><text style="font-size:30rpx;">暂无商品，还有更多好货等着你噢~</text></view></block><block wx:else><view class="order-list"><block wx:for="{{item2.l1}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-every"><view class="every-top"><text class="top-left">{{"订单编号："+item.$orig.outTradeNo}}</text><text class="top-right">{{item.m0}}</text></view><view class="every-main"><block wx:for="{{item.l0}}" wx:for-item="items" wx:for-index="index2" wx:key="index2"><view data-event-opts="{{[['tap',[['goOrderDetail',['$0'],[[['orderAllList','',index2],['orderList','',index,'id']]]]]]]}}" class="main-msg" bindtap="__e"><view class="msg-container"><view class="msg-img"><block wx:if="{{items.$orig.pic}}"><image src="{{items.$orig.pic}}" mode="aspectFill"></image></block></view><view class="msg-data"><view class="data-name">{{items.$orig.productName}}</view><view class="data-price"><text><text style="font-size:24rpx;">CNY</text>{{''+(" "+items.m1)+''}}</text><text style="color:#999999;font-size:26rpx;font-weight:normal;">{{''+('x'+items.$orig.num)}}</text></view><block wx:if="{{item.$orig.orderStatus==1}}"><view style="margin-top:10rpx;"><block wx:if="{{items.$orig.returnMsg}}"><view style="color:#c3c3c3;font-size:22rpx;text-align:right;">该商品已申请退款</view></block><block wx:else><view style="color:#85dbd0;font-size:22rpx;text-align:right;">等待商家发货...</view></block></view></block></view></view><view class="main-money"><text class="money-title">实付款：</text><text class="money"><text style="font-weight:normal;font-size:22rpx;">CNY</text>{{''+(' '+items.m2)}}</text></view></view></block><block wx:if="{{item.$orig.orderStatus==-2||item.$orig.orderStatus==-1}}"><view class="main-button"><view data-event-opts="{{[['tap',[['delOrder',['$0'],[[['orderAllList','',index2],['orderList','',index]]]]]]]}}" class="button-pay" style="background-color:red;" bindtap="__e">删除订单</view></view></block><block wx:if="{{item.$orig.orderStatus===0}}"><view class="main-button"><view data-event-opts="{{[['tap',[['cancelOrder',['$0'],[[['orderAllList','',index2],['orderList','',index,'outTradeNo']]]]]]]}}" class="button-cancel" bindtap="__e">取消订单</view><view data-event-opts="{{[['tap',[['payOrder',['$0'],[[['orderAllList','',index2],['orderList','',index]]]]]]]}}" class="button-pay" bindtap="__e">立即支付</view></view></block><block wx:if="{{item.$orig.orderStatus==2}}"><view class="main-button"><view data-event-opts="{{[['tap',[['logistics',['$0'],[[['orderAllList','',index2],['orderList','',index]]]]]]]}}" class="button-pay" bindtap="__e">查看物流</view><view data-event-opts="{{[['tap',[['deliverySure',['$0'],[[['orderAllList','',index2],['orderList','',index]]]]]]]}}" class="button-pay" bindtap="__e">确认收货</view></view></block><block wx:if="{{item.$orig.orderStatus==3}}"><view class="main-button"><view data-event-opts="{{[['tap',[['delOrder',['$0'],[[['orderAllList','',index2],['orderList','',index]]]]]]]}}" class="button-pay" style="background-color:red;" bindtap="__e">删除订单</view></view></block></view></view></block><block wx:if="{{orderAllList.length>0}}"><view class="is-haveMore">没有更多了~</view></block></view></block></view></scroll-view></swiper-item></block></swiper><uni-popup class="vue-ref" vue-id="01dabdb9-1" type="bottom" data-ref="payPopup" bind:__l="__l" vue-slots="{{['default']}}"><view class="popup-container"><view><view class="popup-title"><text class="currency">CNY</text><text class="price">{{" "+$root.m3}}</text></view><view class="uni-form-item uni-column"><radio-group class="uni-list"><block wx:for="{{radioItems}}" wx:for-item="radio" wx:for-index="index3" wx:key="index3"><label class="uni-list-cell uni-list-cell-pd" style="margin-top:20rpx;display:flex;justify-content:space-between;"><view><label class="label-2-text" style="display:flex;align-items:center;" for="{{radio.name}}"><image style="width:30rpx;height:28rpx;margin-right:10rpx;" src="{{radio.img}}" mode="aspectFill"></image><text style="font-size:28rpx;">{{radio.name}}</text></label></view><view><radio style="transform:scale(0.7);" id="{{radio.name}}" value="{{radio.value}}" checked="{{radio.checked}}"></radio></view></label></block></radio-group></view></view><view data-event-opts="{{[['tap',[['paySure',['$event']]]]]}}" class="button-sure" bindtap="__e">确认支付</view></view></uni-popup></view></block>